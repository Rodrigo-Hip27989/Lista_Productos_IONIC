{
    "terminal.explorerKind": "integrated",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.profiles.linux": {

        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh"
        },
        "fish": {
            "path": "fish"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "android/": true,
        "apks/": true,
        "e2e/": true,
        "node_modules/": true,
        "www/": true,
        ".angular/": true,
        ".browserslistrc": true,
        ".editorconfig": true,
        ".eslintrc.json": true,
        "src/polyfills.ts": true,
        "src/test.ts": true,
        "src/zone-flags.ts": true,
        "angular.json": true,
        "ionic.config.json": true,
        "karma.conf.js": true,
        "License.txt": true,
        "package-lock.json": true,
        "README.md": true,
        "tsconfig.*.*": true,
        "tsconfig.*": true
    }
}